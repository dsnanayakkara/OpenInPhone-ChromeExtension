{
  "manifest_version": 2,
  "version": "1.0.0",

  "name": "OpenInPhone",
  "description": "Open Tab in Phone.Simple plugin to quickly transfer the tab",

  "browser_action": {
    "default_popup": "index.html"
  },
  "icons":{
    "128":"icon128.png",
    "48":"icon48.png",
    "16":"icon16.png"
  },
  "permissions": ["tabs"]
}